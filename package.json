{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron ./lib/main.js",
    "build": "babel ./src/ -d ./lib",
    "test": "ava",
    "watch:test": "ava -w",
    "stylus": "stylus -w ./src/index.styl -o ./lib/index.css"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "./src/setup.js"
    ]
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "browser-env": "^3.1.2",
    "electron": "~1.6.2"
  },
  "dependencies": {
    "stylus": "^0.54.5"
  }
}
